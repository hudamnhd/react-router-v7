    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "execa": "^8.0.1",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "framer-motion": "^11.3.0-alpha.0",
    "fuse.js": "^7.0.0",
    "get-port": "^7.0.0",
    "glob": "^10.3.10",
